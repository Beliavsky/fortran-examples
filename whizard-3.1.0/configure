  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: ./conftest$EXEEXT produced no output" >&5
printf "%s\n" "$as_me: error: ./conftest$EXEEXT produced no output" >&6;}
  as_fn_error $? "***************************************************************************" "$LINENO" 5
  fi
  kinds_error="`$SED -n '/ERROR/s/^.*ERROR: *//p' src/basics/kinds.f90`"
