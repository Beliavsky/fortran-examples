### Find the extension of Fortran module files


{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for Fortran90 module file naming convention" >&5
printf %s "checking for Fortran90 module file naming convention... " >&6; }
cat >conftest.f90 <<__END__
module module_NAME
     implicit none
     integer, parameter, public :: forty_two = 42
   end module module_NAME
program conftest
  print *, 42
end program conftest
__END__
$FC -o conftest conftest.f90 >/dev/null 2>&1
./conftest >conftest.out 2>/dev/null
if test 42 = "`$SED 's/ //g' conftest.out`"; then
  wo_result="ok"
else
  wo_result=""
fi
if test -z "KEEP"; then
  rm -rf conftest* CONFTEST*
fi
if test -n "$wo_result"; then
  FC_MODULE_NAME=unknown
  FCMOD=unknown
  for name in module_name module_NAME MODULE_NAME conftest; do
    for ext in m mod M MOD d D; do
      if test -f "$name.$ext"; then
        FC_MODULE_NAME="$name"
        FCMOD="$ext"
        break 2
      fi
    done
  done
  if test X"$FC_MODULE_NAME" = X"module_name"; then
     { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: name: $FC_MODULE_NAME, extension: .$FCMOD " >&5
