       { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: ***************************************************************************" >&5
printf "%s\n" "$as_me: error: ***************************************************************************" >&6;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: $kinds_error" >&5
printf "%s\n" "$as_me: error: $kinds_error" >&6;}
  as_fn_error $? "***************************************************************************" "$LINENO" 5
  fi
  kinds_warning="`$SED -n '/WARNING/s/^.*WARNING: *//p' src/basics/kinds.f90`"
  if test "x$kinds_warning" != x; then
       { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: ***************************************************************************" >&5
printf "%s\n" "$as_me: WARNING: ***************************************************************************" >&2;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $kinds_warning" >&5
printf "%s\n" "$as_me: WARNING: $kinds_warning" >&2;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: ***************************************************************************" >&5
printf "%s\n" "$as_me: WARNING: ***************************************************************************" >&2;}
  fi
else $as_nop
  rm -f src/basics/kinds.f90
 if test X"$KEEP_KINDLY" != X; then
    cp -a ./conftest.f90 "$KEEP_KINDLY"
 fi
   { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: ***************************************************************************" >&5
printf "%s\n" "$as_me: error: ***************************************************************************" >&6;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: could not compile kinds selection program" >&5
printf "%s\n" "$as_me: error: could not compile kinds selection program" >&6;}
  as_fn_error $? "***************************************************************************" "$LINENO" 5
