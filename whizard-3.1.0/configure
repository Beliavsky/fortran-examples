    enable_looptools=no
fi

    if test "$enable_looptools" = "yes"; then
      LOOPTOOLS_INCLUDES=$wo_looptools_includes
    fi
    ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
