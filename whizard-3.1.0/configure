	 X"src/basics/kinds.f90" : 'X\(//\)$' \| \
	 X"src/basics/kinds.f90" : 'X\(/\)' \| . 2>/dev/null ||
printf "%s\n" X"src/basics/kinds.f90" |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
  ./conftest$EXEEXT "$wo_cv_fc_requested_precision" >src/basics/kinds.f90
  if test X"$KEEP_KINDLY" != X; then
     cp -a ./conftest.f90 "$KEEP_KINDLY"
  fi
  if test ! -s src/basics/kinds.f90; then
       { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: ***************************************************************************" >&5
printf "%s\n" "$as_me: error: ***************************************************************************" >&6;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: ./conftest$EXEEXT produced no output" >&5
printf "%s\n" "$as_me: error: ./conftest$EXEEXT produced no output" >&6;}
  as_fn_error $? "***************************************************************************" "$LINENO" 5
  fi
  kinds_error="`$SED -n '/ERROR/s/^.*ERROR: *//p' src/basics/kinds.f90`"
  if test "x$kinds_error" != x; then
       { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: ***************************************************************************" >&5
printf "%s\n" "$as_me: error: ***************************************************************************" >&6;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: $kinds_error" >&5
printf "%s\n" "$as_me: error: $kinds_error" >&6;}
  as_fn_error $? "***************************************************************************" "$LINENO" 5
  fi
  kinds_warning="`$SED -n '/WARNING/s/^.*WARNING: *//p' src/basics/kinds.f90`"
  if test "x$kinds_warning" != x; then
       { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: ***************************************************************************" >&5
printf "%s\n" "$as_me: WARNING: ***************************************************************************" >&2;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $kinds_warning" >&5
printf "%s\n" "$as_me: WARNING: $kinds_warning" >&2;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: ***************************************************************************" >&5
printf "%s\n" "$as_me: WARNING: ***************************************************************************" >&2;}
  fi
else $as_nop
  rm -f src/basics/kinds.f90
 if test X"$KEEP_KINDLY" != X; then
    cp -a ./conftest.f90 "$KEEP_KINDLY"
 fi
   { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: ***************************************************************************" >&5
printf "%s\n" "$as_me: error: ***************************************************************************" >&6;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: could not compile kinds selection program" >&5
printf "%s\n" "$as_me: error: could not compile kinds selection program" >&6;}
  as_fn_error $? "***************************************************************************" "$LINENO" 5
fi
rm -f core *.core core.conftest.* gmon.out bb.out conftest$ac_exeext \
  conftest.$ac_objext conftest.beam conftest.$ac_ext
fi

else
    if test "$cross_compiling" = yes
then :
    case "X$wo_cv_fc_requested_precision" in
    Xsingle)
      wo_cv_fc_requested_default_c_float_kind=c_float
      wo_cv_fc_requested_default_c_complex_kind=c_float_complex
      ;;
    Xdouble)
      wo_cv_fc_requested_default_c_float_kind=c_double
      wo_cv_fc_requested_default_c_complex_kind=c_double_complex
      ;;
    *)
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: ***************************************************************************" >&5
printf "%s\n" "$as_me: error: ***************************************************************************" >&6;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error:         cross compilation supported for single and double precision only!" >&5
printf "%s\n" "$as_me: error:         cross compilation supported for single and double precision only!" >&6;}
  as_fn_error $? "***************************************************************************" "$LINENO" 5
      ;;
  esac
  cat > conftest.$ac_ext <<_ACEOF
    ! A minimalistic kinds.f90, since we're cross compiling
module kinds
  use, intrinsic :: iso_fortran_env
  use, intrinsic :: iso_c_binding
  implicit none
  private
  public :: single, double
  integer, parameter :: single = kind (1.0)
  integer, parameter :: double = kind (1.0D0)
  public :: i8, i16, i32, i64, TC
  integer, parameter :: i8  = selected_int_kind (2)
  integer, parameter :: i16 = selected_int_kind (4)
  integer, parameter :: i32 = selected_int_kind (9)
  integer, parameter :: i64 = selected_int_kind (18)
  integer, parameter :: TC = i32
  public :: default
  integer, parameter :: default =        $wo_cv_fc_requested_precision
  public :: c_default_float, c_default_complex
  integer, parameter :: c_default_float = $wo_cv_fc_requested_default_c_float_kind
  integer, parameter :: c_default_complex = $wo_cv_fc_requested_default_c_complex_kind
