  FCMOD=""
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: compiler failed" >&5
printf "%s\n" "compiler failed" >&6; }
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: **************************************************************" >&5
printf "%s\n" "$as_me: error: **************************************************************" >&6;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: Fortran compiler cannot create proper module files. This" >&5
printf "%s\n" "$as_me: error: Fortran compiler cannot create proper module files. This" >&6;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: might be caused by linking against a wrong gcc library." >&5
printf "%s\n" "$as_me: error: might be caused by linking against a wrong gcc library." >&6;}
  as_fn_error $? "**************************************************************" "$LINENO" 5
